generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Escala {
  id          Int      @id @default(autoincrement())
  name        String
  escala      String
  status      Status
  description String[]
  pendencias  String[]

  @@map("escalas")
}

enum Status {
  estavel
  urgente
}
